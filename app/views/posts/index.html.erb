<h1>LYRICS FEED</h1>

<% @posts.each do |post| %>
div>
	<h2><%= post.user %></h2>
<h2><%= post.text %></h2>
<h3><%= post.song.artist %></h3>
<h3><%= post.song.title %></h3>
<h3><%= post.song.lyric %></h3>
<h4><%= post.comments.map {|comment| comment.comment}.join(", ") %></h4>
<h5><%= post.commenters.map {|commenter| commenter.name}.join(", ") %></h5>
</div>
<% end %>

<%= link_to 'Create New Post', new_post, method: :get %> 